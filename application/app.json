{
  "expo": {
    "name": "Ankan's Portfolio",
    "slug": "ankanhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Myself.png",
    "scheme": "application",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theankansaha.ankanhub",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Myself.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.theankansaha.ankanhub"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Myself.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Myself.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9d699b2c-fa5a-4e56-9ef8-1bdf6970add6"
      }
    },
    "owner": "theankansaha"
  }
}